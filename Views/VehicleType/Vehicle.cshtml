@model UtilizationTrackerApp.Models.Data.VehicleType_Model
@{
    ViewBag.Title = "Tax";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Tax Master</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">TaxMaster</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
